{
	"name": "cakestall",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/cakestall/src",
	"projectType": "library",
	"targets": {
		"prepublish": {
			"dependsOn": ["build"],
			"command": "node tools/scripts/postbuild.mjs cakestall {args.ver} {args.tag}"
		},
		"build": {
			"executor": "@nx/rollup:rollup",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"main": "packages/cakestall/src/index.ts",
				"outputPath": "dist/packages/cakestall",
				"tsConfig": "packages/cakestall/tsconfig.lib.json",
				"compiler": "babel",
				"project": "packages/cakestall/package.json",
				"format": ["esm"],
				"rollupConfig": "packages/cakestall/rollup.cjs",
				"assets": []
			},
			"configurations": {
				"production": {
					"optimization": true,
					"sourceMap": false,
					"namedChunks": false,
					"extractLicenses": true,
					"vendorChunk": false
				}
			}
		}
	},
	"tags": []
}
